<%= turbo_frame_tag "search_bar" do %>
    <%= form_with url: search_entries_index_url, data: { controller: "search" } do |form| %>
      <%= form.text_field :query, data: {target: "search.query", action: "input->search#search"} %>
    <% end %>

  <%= render "search_entries/results" %>
  <%= turbo_stream_from "search_results" %>
<% end %>
